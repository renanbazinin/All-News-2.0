{
  "expo": {
    "name": "new",
    "slug": "new",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/myLogo.png",
    "scheme": "new",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0a0a0a",
        "foregroundImage": "./assets/images/myLogo.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.renanbazinin2.newsapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/myLogo.png",
      "bundler": "metro",
      "publicPath": "/All-News-2.0/"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/myLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0a0a0a",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {
        "origin": "hash",
        "basePath": "/All-News-2.0"
      },
      "eas": {
        "projectId": "d37c15f3-9565-47fc-b941-7a1fce0e98cc"
      }
    }
  }
}
